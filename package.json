{
  "name": "aws-lambda-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Imamachi-n/aws-lambda-typescript.git",
  "author": "Imamachi-n <naoto.imamachi@cureapp.jp>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -r dist && rm -r dist.zip",
    "ts": " tsc -p tsconfig.json",
    "cp": "cp -f ./package.json ./dist",
    "install": "npm --prefix ./dist install --production ./dist",
    "zip": "cd dist/ && bestzip ../dist.zip *",
    "build": "yarn clean && yarn ts && yarn cp && yarn install && yarn zip"
  },
  "devDependencies": {
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "moment": "^2.27.0"
  }
}
